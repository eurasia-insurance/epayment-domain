CREATE TABLE INVOICE (ID INTEGER AUTO_INCREMENT NOT NULL, PAYER_EMAIL VARCHAR(255), CONSUMER_NAME VARCHAR(255), CONSUMER_PHONE VARCHAR(255), CONSUMER_PREFER_LANGUAGE VARCHAR(255), CONSUMER_TAXPAYER_NUMBER VARCHAR(255), CREATED DATETIME, CURRENCY VARCHAR(255), EXPIRED DATETIME, EXTERNAL_ID VARCHAR(255), NUMBER VARCHAR(255) UNIQUE, PAYMENT_ID INTEGER, PRIMARY KEY (ID))
