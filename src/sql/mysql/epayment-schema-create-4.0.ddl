CREATE TABLE QAZKOM_ORDER (ID INTEGER AUTO_INCREMENT NOT NULL, AMOUNT DOUBLE, CREATED DATETIME, CURRENCY VARCHAR(255), ORDER_NUMBER VARCHAR(255) UNIQUE, CART_DOC_ID INTEGER, INVOICE_ID INTEGER, ORDER_DOC_ID INTEGER, PAYMENT_ID INTEGER, PRIMARY KEY (ID))
